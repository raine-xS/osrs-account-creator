{
  "manifest_version": 3,
  "name": "Disable Content Security Policy",
  "version": "1.0.0",
  "description": "Disables CSP headers for all web pages.",
  "permissions": [
    "declarativeNetRequest"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "128": "/img/128.png"
  }
}
